{
  "api": {
    "user": "foo",
    "password": "bar"
  },
  "filters": {
    "production": {
      "attributes": {
        "client": {
          "environment": "production"
        }
      }
    },
    "development": {
      "negate": true,
      "attributes": {
        "client": {
          "environment": "production"
        }
      }
    }
  },
  "mutators": {
    "tag": {
      "command": "./spec/mutators/tag.rb"
    },
    "explode": {
      "command": "echo -n exploded && exit 2"
    }
  },
  "handlers": {
    "default": {
      "type": "set",
      "handlers": [
        "debug"
      ]
    },
    "keepalive": {
      "type": "set",
      "handlers": [
        "debug"
      ]
    },
    "registration": {
      "type": "pipe",
      "command": "cat > /tmp/sensu_client_registration.json"
    },
    "file": {
      "type": "pipe",
      "command": "cat > /tmp/sensu_event"
    },
    "filtered": {
      "type": "pipe",
      "command": "cat > /tmp/sensu_event",
      "filter": "development"
    },
    "severities": {
      "type": "pipe",
      "command": "cat > /tmp/sensu_event",
      "severities": [
        "critical",
        "unknown"
      ]
    },
    "flapping": {
      "type": "pipe",
      "command": "cat > /tmp/sensu_event",
      "handle_flapping": true
    },
    "tcp": {
      "type": "tcp",
      "socket": {
        "host": "localhost",
        "port": 1234
      }
    },
    "udp": {
      "type": "udp",
      "socket": {
        "host": "localhost",
        "port": 1234
      }
    },
    "transport": {
      "type": "transport",
      "pipe": {
        "type": "direct",
        "name": "events"
      }
    },
    "nested_set_one": {
        "type": "set",
        "handlers": [
            "nested_set_two"
        ]
    },
    "nested_set_two": {
        "type": "set",
        "handlers": [
            "debug",
            "file",
            "nested_set_three"
        ]
    },
    "nested_set_three": {
        "type": "set",
        "handlers": [
            "udp",
            "tcp",
            "transport"
        ]
    }
  },
  "checks": {
    "tokens": {
      "command": "echo -n :::name::: :::nested.attribute::: && exit 2",
      "subscribers": [
        "test"
      ],
      "interval": 1
    },
    "standalone": {
      "command": "echo -n foobar && exit 1",
      "standalone": true,
      "interval": 1,
      "ttl": 10
    },
    "merger": {
      "command": "this will be overwritten",
      "interval": 60
    },
    "source": {
      "source": "switch-y",
      "command": "/bin/true",
      "subscribers": [
        "test"
      ],
      "interval": 1
    },
    "roundrobin": {
      "command": "/bin/true",
      "subscribers": [
        "roundrobin:test"
      ],
      "interval": 1
    },
    "unpublished": {
      "command": "/bin/true",
      "publish": false,
      "subscribers": [
        "test"
      ]
    }
  },
  "client": {
    "name": "i-424242",
    "address": "127.0.0.1",
    "subscriptions": [
      "test",
      "roundrobin:test",
      "all"
    ],
    "keepalive": {
      "thresholds": {
        "warning": 10
      }
    },
    "deregister": true,
    "deregistration": {
      "handler": "DEREGISTER_HANDLER"
    },
    "version": "TOBEREPLACED",
    "nested": {
      "attribute": true
    },
    "service": {
      "password": "secret"
    },
    "empty": {},
    "localhost": "fail"
  }
}
