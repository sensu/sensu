{
  "api": {
    "host": "localhost",
    "port": 4567,
    "user": "foo",
    "password": "bar"
  },
  "filters": {
    "production": {
      "attributes": {
        "client": {
          "environment": "production"
        }
      }
    },
    "development": {
      "negate": true,
      "attributes": {
        "client": {
          "environment": "production"
        }
      }
    }
  },
  "mutators": {
    "tag": {
      "command": "./spec/mutators/tag.rb"
    },
    "explode": {
      "command": "echo -n exploded && exit 2"
    }
  },
  "handlers": {
    "default": {
      "type": "set",
      "handlers": [
        "debug"
      ]
    },
    "file": {
      "type": "pipe",
      "command": "cat > /tmp/sensu_event"
    },
    "filtered": {
      "type": "pipe",
      "command": "cat > /tmp/sensu_event",
      "filter": "development"
    },
    "severities": {
      "type": "pipe",
      "command": "cat > /tmp/sensu_event",
      "severities": [
        "critical",
        "unknown"
      ]
    },
    "flapping": {
      "type": "pipe",
      "command": "cat > /tmp/sensu_event",
      "handle_flapping": true
    },
    "tcp": {
      "type": "tcp",
      "socket": {
        "host": "localhost",
        "port": 1234
      }
    },
    "udp": {
      "type": "udp",
      "socket": {
        "host": "localhost",
        "port": 1234
      }
    },
    "transport": {
      "type": "transport",
      "pipe": {
        "type": "direct",
        "name": "events"
      }
    }
  },
  "checks": {
    "tokens": {
      "command": "echo -n :::name::: :::nested.attribute::: && exit 2",
      "subscribers": [
        "test"
      ],
      "interval": 1
    },
    "standalone": {
      "command": "echo -n foobar && exit 1",
      "standalone": true,
      "interval": 1
    },
    "merger": {
      "command": "this will be overwritten",
      "interval": 60
    },
    "unpublished": {
      "command": "/bin/true",
      "publish": false,
      "subscribers":["test"]
    }
  },
  "client": {
    "name": "i-424242",
    "address": "127.0.0.1",
    "subscriptions": [
      "test"
    ],
    "nested": {
      "attribute": true
    },
    "service": {
      "password": "secret"
    },
    "keepalive": {
      "thresholds": {
        "warning": 10
      }
    }
  }
}
