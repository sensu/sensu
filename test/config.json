{
  "rabbitmq": {
    "host": "localhost",
    "port": 5672
  },
  "redis": {
    "host": "localhost",
    "port": 6379
  },
  "api": {
    "host": "localhost",
    "port": 7070
  },
  "dashboard": {
    "host": "localhost",
    "port": 8080,
    "user": "admin",
    "password": "secret"
  },
  "handlers": {
    "default": "ruby ./test/handler.rb"
  },
  "checks": {
    "a": {
      "command": "ruby -e \"puts ':::name:::'; exit 1\"",
      "subscribers": [
        "a"
      ],
      "interval": 1
    },
    "b": {
      "command": "will be overridden by a config snippet",
      "subscribers": [
        "a"
      ],
      "interval": 60,
      "auto_resolve": true
    },
    "c": {
      "handler": "default",
      "command": "ruby -e \"puts ':::name:::'; exit 3\"",
      "subscribers": [
        {
          "name": "i-424242",
          "subscribes": "a"
        }
      ],
      "interval": 1
    },
    "d": {
      "handlers": [
        "default"
      ],
      "command": "ruby -e \"puts ':::name:::'; exit 4\"",
      "subscribers": [
        "a",
        "b",
        "c"
      ],
      "interval": 1
    }
  },
  "client": {
    "name": "i-424242",
    "address": "127.0.0.1",
    "subscriptions": [
      "a",
      "b",
      "c"
    ]
  }
}
