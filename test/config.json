{
  "rabbitmq": {
    "host": "localhost",
    "port": 5672
  },
  "redis": {
    "host": "localhost",
    "port": 6379
  },
  "api": {
    "host": "localhost",
    "port": 4567
  },
  "dashboard": {
    "host": "localhost",
    "port": 8080,
    "user": "admin",
    "password": "secret"
  },
  "handlers": {
    "default": {
      "type": "set",
      "handlers": [
        "debug"
      ]
    },
    "debug": {
      "type": "pipe",
      "command": "./test/handler.rb"
    },
    "graphite": {
      "type": "amqp",
      "exchange": {
        "type": "topic",
        "name": "graphite",
        "key": "sensu.test"
      },
      "send_only_check_output": true
    }
  },
  "checks": {
    "a": {
      "command": "ruby -e \"puts ':::name::: :::custom.nested.attribute:::'; exit 1\"",
      "subscribers": [
        "a"
      ],
      "interval": 1
    },
    "b": {
      "command": "will be overridden by a config snippet",
      "subscribers": [
        "a"
      ],
      "interval": 60,
      "auto_resolve": true
    },
    "c": {
      "handler": "default",
      "command": "ruby -e \"puts ':::name::: :::custom.nested.attribute:::'; exit 3\"",
      "standalone": true,
      "interval": 1
    },
    "d": {
      "handlers": [
        "default"
      ],
      "command": "ruby -e \"puts ':::name::: :::custom.nested.attribute:::'; exit 4\"",
      "subscribers": [
        "a",
        "b",
        "c"
      ],
      "interval": 1
    },
    "e": {
      "type": "metric",
      "handler": "graphite",
      "command": "echo sensu.:::name:::.diceroll 4 `date +%s`",
      "subscribers": [
        "c"
      ],
      "interval": 1
    }
  },
  "client": {
    "name": "i-424242",
    "address": "127.0.0.1",
    "subscriptions": [
      "a",
      "b",
      "c"
    ],
    "custom": {
      "nested": {
        "attribute": true
      }
    }
  }
}
