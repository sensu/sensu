{
  "rabbitmq": {
    "host": "localhost",
    "port": 5672
  },
  "redis": {
    "host": "localhost",
    "port": 6379
  },
  "api": {
    "host": "localhost",
    "port": 4567,
    "user": "foo",
    "password": "bar"
  },
  "mutators": {
    "tag": {
      "command": "./test/mutators/tag.rb"
    }
  },
  "handlers": {
    "default": {
      "type": "set",
      "handlers": [
        "stdout"
      ]
    },
    "stdout": {
      "type": "pipe",
      "command": "./test/handlers/stdout.rb"
    },
    "file": {
      "type": "pipe",
      "command": "./test/handlers/file.rb"
    },
    "tcp_socket": {
      "type": "tcp",
      "socket": {
        "host": "localhost",
        "port": 1234
      }
    },
    "udp_socket": {
      "type": "udp",
      "socket": {
        "host": "localhost",
        "port": 1234
      }
    },
    "amqp_exchange": {
      "type": "amqp",
      "exchange": {
        "name": "events"
      }
    },
    "tagged": {
      "type": "pipe",
      "mutator": "tag",
      "command": "./test/handlers/file.rb"
    },
    "missing_mutator": {
      "type": "pipe",
      "mutator": "missing",
      "command": "./test/handlers/file.rb"
    },
    "only_output": {
      "type": "pipe",
      "mutator": "only_check_output",
      "command": "cat"
    },
    "only_output_split": {
      "type": "amqp",
      "mutator": "only_check_output_split",
      "exchange": {
        "name": "sensu"
      }
    }
  },
  "checks": {
    "standalone": {
      "command": "echo -n :::name::: && exit 1",
      "standalone": true,
      "interval": 1
    },
    "tokens": {
      "command": "echo -n :::name::: :::nested.attribute::: && exit 2",
      "subscribers": [
        "test"
      ],
      "interval": 1
    },
    "tokens_fail": {
      "command": "echo -n :::name::: :::nested.foo.bar::: && exit 2",
      "subscribers": [
        "test"
      ],
      "interval": 1
    },
    "merger": {
      "command": "exit",
      "subscribers": [
        "test"
      ],
      "interval": 1
    },
    "aggregated": {
      "command": "echo -n foobar.local",
      "subscribers": [
        "test"
      ],
      "aggregate": true,
      "interval": 1
    }
  },
  "client": {
    "name": "i-424242",
    "address": "127.0.0.1",
    "subscriptions": [
      "test"
    ],
    "nested": {
      "attribute": true
    }
  }
}
