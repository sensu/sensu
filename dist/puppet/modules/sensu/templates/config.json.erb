{
  "rabbitmq": {
    "host": "<%= rabbitmq_host %>",
    "port": <%= rabbitmq_port %>
  },
  "redis": {
    "host": "<%= redis_host %>",
    "port": <%= redis_port %>
  },
  "api": {
    "host": "<%= api_host %>",
    "port": <%= api_port %>
  },
  "handlers": {
    "default": "/etc/sensu/handlers/default"
  },
  "dashboard": {
    "host": "<%= dashboard_host %>",
    "port": <%= dashboard_port %>,
    "user": "<%= dashboard_user %>",
    "password": "<%= dashboard_password %>"
  },
  "checks": {
    "puppet_agent": {
      "notification": "Puppet agent daemon is not running.",
      "command": "/etc/sensu/plugins/puppet_agent.rb",
      "subscribers": [ "puppet_agent" ],
      "interval": 60
    }
  },
  "client": {
    "name": "<%= hostname %>",
    "address": "<%= ipaddress %>",
    "subscriptions": [ "puppet_agent" ]
  }
}
